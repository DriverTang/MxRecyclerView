<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.marst.recyclerview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".linearlayout.LinearLayoutActivity" />
        <activity android:name=".gridlayout.GridLayoutActivity" />
        <activity android:name=".staggered.StaggeredGridActivity" />
        <activity android:name=".itemclick.ItemClickActivity" />
        <activity android:name=".header.HeaderActivity" />
        <activity android:name=".divider.DividerActivity" />
        <activity android:name=".swiprefresh.SwipRefreshActivity"></activity>
    </application>

</manifest>